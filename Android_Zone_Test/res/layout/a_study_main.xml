<?xml version="1.0" encoding="utf-8"?>
<com.zone.view.FlowLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/flowLayoutZone1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        style="@style/TvHint"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="文件打开研究" />

    <TextView
        android:id="@+id/android_pdf"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Android PDF" />

    <TextView
        android:id="@+id/pdfbox"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PDFBox" />

    <TextView
        android:id="@+id/x5_open_office"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="X5 open Office" />

    <TextView
        android:id="@+id/google_office"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Google Office" />

    <TextView
        style="@style/TvHint"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="other提示" />

    <TextView
        android:id="@+id/tips"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tips测试" />

    <TextView
        android:id="@+id/dynamicLogo"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="动态图标测试" />

    <TextView
        android:id="@+id/bt_data_crash"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="程序杀死崩溃存储信息" />


    <TextView
        android:id="@+id/bt_crash"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="崩溃！" />

    <TextView
        android:id="@+id/bt_display_cutout"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="刘海屏适配研究！" />
    <TextView
        style="@style/TvHint"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="other提示" />

    <TextView
        android:id="@+id/bt_crash"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HotfixTest" />

    <TextView
        android:id="@+id/bt_song_scroll"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="歌词滚播" />

    <TextView
        android:id="@+id/bt_drag_view"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="锁定框内拖拽view" />

    <TextView
        style="@style/TvHint"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="service" />


    <TextView
        android:id="@+id/bt_service"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="startservice" />


    <TextView
        android:id="@+id/bt_NotifiBindservice"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NotifiBindservice" />


    <TextView
        android:id="@+id/bt_NotifiHideservice"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NotifiHideservice" />


    <TextView
        android:id="@+id/bt_ky_ad_view"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ky广告动态布局" />


    <TextView
        android:id="@+id/bt_list_filter"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RV过滤吸附列表" />

    <TextView
        android:id="@+id/bt_ky_scrollTop"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ky嵌套滚动到顶部" />


    <TextView
        android:id="@+id/bt_ky_scrollTop_appbar"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="kyAppbar嵌套滚动到顶部" />


    <TextView
        android:id="@+id/bt_a_hashcode"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Activity的hashcode" />

    <TextView
        android:id="@+id/bt_go_this"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转当前页面" />

    <TextView
        android:id="@+id/bt_rv_hori"
        style="@style/TvOnClick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="横向滚动RV" />
</com.zone.view.FlowLayout>